        <!-- Page Content -->
        
        <div id="course-page-content-wrapper" class="col-md-10">
          <%=link_to "Sign out", destroy_admin_session_path , :method => :delete %>
          <%= form_tag('', method: "post", :id => "course-create-form" ) do %>
            
            <div class="col-md-12">
                <div class="col-md-12 course-info-row">
                    <div class="col-md-4">
                      <%= label_tag :name ,'Name',class:"col-md-12 zero-left-right-padd"%>
                      <%= text_field_tag('name', value = nil, class:"col-md-12", placeholder: "Full name of course")%>
                    </div>

                    <div class="col-md-8">
                        <div class="col-md-3">
                          <%= label_tag :location ,'Location',class:"col-md-12 zero-left-right-padd"%>
                          <%= text_field_tag('town', value = nil, class:"col-md-12", placeholder: "Town")%>
                        </div>

                        <div class="col-md-3">
                          <%= select_tag('state', option_tags = nil, options = {prompt: 'State', class:'dropdown-less-padd state-drop-dwn' }) %>
                        </div>

                        <div class="col-md-3">
                          <%= label_tag :lattitude ,'lattitude',class:"col-md-12 zero-left-right-padd"%>
                          <%= text_field_tag('name', value = nil, class:"col-md-12", placeholder: "Lattitude")%>
                    
                        </div>

                        <div class="col-md-3">
                          <%= label_tag :longitude ,'Longitude',class:"col-md-12 zero-left-right-padd"%>
                          <%= text_field_tag('longitude', value = nil, class:"col-md-12", placeholder: "Longitude")%>
                    
                        </div>
                    </div>

                </div>
                
                <div class="col-md-12 course-info-row">
                    <div class="col-md-4">
                      <%= label_tag :course_type ,'Course Type',class:"col-md-12 zero-left-right-padd"%>
                      <%= check_box_tag 'public'%> Public
                      <%= check_box_tag 'private'%> Private
                      <%= check_box_tag 'semi-private'%> Semi-Private
                    
                    </div>

                    <div class="col-md-4">
                      <%= select_tag('resorts', option_tags = nil, options = {prompt: 'Resort', class:'dropdown-padd'}) %>
                    </div>

                    <div class="col-md-4">
                      <%= select_tag('network', option_tags = nil, options = {prompt: 'Network',class:'dropdown-padd'}) %>
                    </div>
                </div>

                <div class="col-md-12 course-info-row">
                    <div class="col-md-5">
                      <%= label_tag :bio ,'Bio',class:"col-md-12 zero-left-right-padd"%>
                      <%= text_area('', '', options = {rows:"4", class:'col-md-12'})%>
                    
                    </div>

                    <div class="col-md-3">
                      <%= label_tag :contact_info ,'Contact Info',class:"col-md-12 zero-left-right-padd"%>
                      <%= text_field_tag('website', value = nil, class:"col-md-12", placeholder: "Website")%>
                      <%= text_field_tag('phone_number', value = nil, class:"col-md-12", placeholder: "Phone Number")%>
                    </div>

                    <div class="col-md-4">
                      <%= label_tag :amenties ,'Amenties',class:"col-md-12 zero-left-right-padd"%>
                      <%= check_box_tag 'restaurant'%> Restaurant
                      <%= check_box_tag 'caddies'%> Caddies
                      <%= check_box_tag 'carts'%> Carts
                    
                    </div>
                    
                </div>

                <div class="col-md-12 course-info-row">
                    <div class="col-md-2">
                      <%= label_tag :logo ,'Logo',class:"col-md-12 zero-left-right-padd"%>
                      <button type="button" class="btn btn-success upload-btn">Upload</button>
                    </div>

                    <div class="col-md-2">
                      <%= label_tag :cover ,'Cover Photo',class:"col-md-12 zero-left-right-padd"%>
                      <button type="button" class="btn btn-success upload-btn">Upload</button>
                    </div>

                    <div class="col-md-8"></div>
                    
                </div>
                
                <div class="col-md-12 course-info-row">
                    <div class="col-md-2">
                      <%= label_tag :total_par ,'Total Par',class:"col-md-12 zero-left-right-padd"%>
                      <%= text_field_tag('total_par', value = nil, class:"col-md-12", placeholder: "Total Par")%>
                    </div>

                    <div class="col-md-2">
                      <%= label_tag :slope ,'Slope',class:"col-md-12 zero-left-right-padd"%>
                      <%= text_field_tag('slope', value = nil, class:"col-md-12", placeholder: "Slope")%>
                    </div>

                    <div class="col-md-2">
                      <%= label_tag :rating ,'Rating',class:"col-md-12 zero-left-right-padd"%>
                      <%= text_field_tag('rating', value = nil, class:"col-md-12", placeholder: "Rating")%>
                    </div>

                    <div class="col-md-2">
                      <%= label_tag :length ,'Length',class:"col-md-12 zero-left-right-padd"%>
                      <%= text_field_tag('length', value = nil, class:"col-md-12", placeholder: "Length")%>
                    </div>
                </div>

                <div class="col-md-12 course-info-row">
                    <div class="col-md-8">
                      <%= label_tag :score_card ,'Score Card',class:"col-md-12 zero-left-right-padd"%>
                      <%= text_field_tag('tee_name', value = nil, class:"col-md-3", placeholder: "Tee Name")%>
                      <%= select_tag('color', option_tags = nil, options = {prompt: 'Color',class:'dropdown-less-padd col-md-3' ,style: 'margin-left:10px;'}) %>
                    </div>

                    <div class="col-md-4">
                      
                    </div>
                    
                </div>

                <div class="col-md-12 course-info-row">
                    
                    <%1..18.times do |index|%> 
                      <%=render :partial => 'hole' , locals: { :num => index + 1 } %>
                    <%end%>

                    
                    
                </div>


            </div>
          <%end%>
