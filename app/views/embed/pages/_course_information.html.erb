<script type="text/javascript">
$(document).ready(function(){
  $('.bxslider').bxSlider({
    mode: 'horizontal',
    useCSS: false,
    // infiniteLoop: false,
    // hideControlOnEnd: true,
    // // easing: 'easeOutElastic',
    // speed: 2000,
      pager: false,
      nextText: '<%= image_tag "right.png" %>',
      prevText: '<%= image_tag "left.png" %>'
   });


  $('.responsive').slick({
    dots: false,
    infinite: false,
    speed: 300,
    slidesToShow: 6,
    slidesToScroll: 1,
    responsive: [
      {
        breakpoint: 1024,
        settings: {
          slidesToShow: 3,
          slidesToScroll: 3,
          infinite: true,
          dots: true
        }
      },
      {
        breakpoint: 600,
        settings: {
          slidesToShow: 3,
          slidesToScroll: 1
        }
      },
      {
        breakpoint: 480,
        settings: {
          slidesToShow: 2,
          slidesToScroll: 2
        }
      }
    ]
});

});

$('#select_map').click(function (){
  bgColor=$(this).attr('color');
  $('.st1 .bx-wrapper').invisible();
  // $('#slider').hide();
  $('#select_photos').removeClass('selected');
  $('#select_photos').addClass('non-selected');
  $('#select_photos').css({"background-color":"white"});

  $('#map').show();
  $('#select_map').removeClass('non-selected');
  $('#select_map').addClass('selected');
  $('#select_map').css({"background-color":bgColor});

  // $('.bx-next').hide();
  // $('.bx-prev').hide();
});

$('#select_photos').click(function (){
  bgColor=$(this).attr('color');
  $('#map').hide();
  $('#select_map').removeClass('selected');
  $('#select_map').addClass('non-selected');
  $('#select_map').css({"background-color":"white"});

  $('.st1 .bx-wrapper').visible();
  // $('#slider').show();
  $('#select_photos').removeClass('non-selected');
  $('#select_photos').addClass('selected');
  $('#select_photos').css({"background-color":bgColor});
  // $('.bx-next').show();
  // $('.bx-prev').show();
});


(function($) {
    $.fn.invisible = function() {
        return this.each(function() {
            $(this).css("visibility", "hidden");
        });
    };
    $.fn.visible = function() {
        return this.each(function() {
            $(this).css("visibility", "visible");
        });
    };
}(jQuery));


$(document).ready(function() { $('#select_photos').trigger('click') });
