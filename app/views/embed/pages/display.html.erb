// trigger for first ajax call
$(document).ready(function() { $('#1 a').trigger('click') });

// Pgae Refresher for safari cache
$(window).bind("pageshow", function(event) {
    if (event.originalEvent.persisted) {
        window.location.reload() 
    }
});


