<script type="text/javascript">



var videoList = []

for (var i = 0, len = gon.videos_urls.length; i < len; i++) {
  if (i==0){ // setting poster for only first video
    videoList.push({
    sources: [{
      src: gon.videos_urls[i],
      type: 'video/mp4'
    }],
    poster: gon.image_urls[i]
  });

  }else{ // Playlist videos
    videoList.push({
      sources: [{
        src: gon.videos_urls[i],
          type: 'video/mp4'
      }]
    });
  }
}

// 320, 480, 760, 960, 1200 and 1600px.
var player = videojs('hole-vid', {
  preload: 'auto', inactivityTimeout: 5000, controls: true, fluid:true,
  'playbackRates': [0.5, 1, 1.5, 2],
   controlBar: {
    children: {
      'playToggle':{},
      'progressControl':{},
      'volumeMenuButton':{},      
      'playbackRateMenuButton':{},
      // 'settingsMenuButton': {
      //     entries : [
      //       'subtitlesButton',
      //       'playbackRateMenuButton'
      //     ]
      //   },
      'fullscreenToggle':{}
    } }
});

try {player.volume(1);} catch (e) {}

player.playlist(videoList);
player.playlist.autoadvance(0);

document.querySelector('.previous').addEventListener('click', function() {
  // i = player.playlist.currentItem();
  player.playlist.previous();
  // $('#'+i+' a').trigger('click');
});
document.querySelector('.next').addEventListener('click', function() {
  // i = player.playlist.currentItem()+2;
  player.playlist.next();
  // $('#'+i+' a').trigger('click');
});

// document.querySelector('.flag').addEventListener('click', function() {
//   flag_control();
  
// });

function flag_control(){
  for (var i = 0, len = gon.videos.length; i < len; i++) {
    if (gon.tags[i].length > 0) {
      if (player.currentSrc().includes(gon.videos[i].video_file_name) && gon.videos[i].id == gon.tags[i][0].video_id) {
        player.currentTime(gon.tags[i][0].time);
      }
    }
  }
}


var timer;

player.on("loadedmetadata",function () {
  barupdate();
  current_video();

  barShow();
  
  
  // $('#video').trigger('mousemove');
  // player.trigger('useractive');
  // setTimeout(trg, 5000);
});
function barShow(){
  if (!player.paused()){
    showBars();
    // $(".hole-bar").animate({top: '1%'});
    timer = setTimeout(function(){barHide()}, 5000);
  }
}
function barHide(){
  hideBars();
  // $(".hole-bar").animate({top: '-70px'});
}

player.on("userinactive",function () {
  if (!player.paused()){
    hideBars();
  }
});

player.on("useractive",function () {

  if (!player.paused()){
    showBars();
    clearTimeout(timer);
  }
});

player.on ("error",function () {
    // alert(player.error.message);
    // player.playlist.next();
  });

function hideBars(){
    $(".hole-bar").animate({top: '-70px'});
    $(".logo").animate({top: '-70px'});
    $(".previous").fadeOut();
    $(".next").fadeOut();
    $(".vjs-control-bar").animate({bottom: '-70px'});
    $(".upper-gradient").animate({top: '-70px'});
    // $(".lower-gradient").animate({bottom: '0px'});
    $(".lower-gradient").slideUp(300);
}

function showBars(){
    $(".hole-bar").animate({top: '1%'});
    $(".logo").animate({top: '0'});
    $(".previous").fadeIn();
    $(".next").fadeIn();
    $(".vjs-control-bar").animate({bottom: '1px'});
    $(".upper-gradient").animate({top: '0px'});
    // xx = $(".vjs-playlist").outerHeight();
    // // xx-=2;
    // $(".lower-gradient").show().animate({bottom: xx+'px'});
    $(".lower-gradient").slideDown(500);

}

/* ADD flag */
var Button = videojs.getComponent('Button');

var FlagButton = videojs.extend(Button, {
  constructor: function() {
    Button.apply(this, arguments);
    this.addClass('flag');
    this.controlText("Flag");
  },

  handleClick: function() {
    flag_control();
  }
});

// Register the new component
videojs.registerComponent('FlagButton', FlagButton);
player.getChild('controlBar').addChild('FlagButton', {}, 4);


function playlistui(){
  // for (var i = 0, len = gon.videos_urls.length; i < len; i++) {

    // document.getElementById('playlist').innerHTML = '<li id="'+0+'" class="hole-home" onClick="select_video(this.id)"></li>';

    for (var i = 1, len = 19; i < len; i++) {
      document.getElementById('playlist').innerHTML += '<li id="'+i+'" onClick="select_video(this.id)">'+i+'</li>';
    }
}

function barupdate(){
  var hole = player.playlist.currentItem();
  
  document.getElementById('hole').innerHTML = leftPad(hole+1,1);
  document.getElementById('hole-num').innerHTML = pos(hole+1);
  document.getElementById('par').innerHTML = leftPad(gon.par[hole],1);
  document.getElementById('yard').innerHTML = leftPad(gon.yard[hole],1);
  document.getElementById('mhcp').innerHTML = leftPad(gon.mhcp[hole],1);

}

function leftPad(number, targetLength) {
    var output = number + '';
    while (output.length < targetLength) {
        output = '0' + output;
    }
    return output;
}

function pos(num){
  switch (num) {
    default:
        return "TH";
        break;
    case 1:
        return "ST";
        break;
    case 2:
        return "ND";
        break;
    case 3:
        return "RD";
  }
}



function select_video(id){
  if (id==='0'){
    $('#0').addClass("active");
  }
  else{
    id-=1;
    player.playlist.currentItem(id);
  }
}
  
$(document).ready(function(){
    $(".right-green").click(function(){
        // alert($(".playlist").scrollLeft() + " px");
        // alert($(".playlist ul li").outerWidth(true) + " px");
    });
});

function scrollTo(a){
  // currPos = $(".playlist").scrollLeft();
  liWidth = $(".playlist ul li").outerWidth(true);
  scrollValue = a*liWidth;
  // $('.playlist').animate( { scrollLeft: '0' }, 0);
  $('.playlist').animate( { scrollLeft: scrollValue }, 500);

}

function current_video(){
  $('#0').removeClass("active");
  id = player.playlist.currentItem();
  for (i=0;i<18;i++){
    if (i===id){
      document.getElementById(i+1).className = "active";
      j=i+1;
      $('#'+j+' a').trigger('click');
      scrollTo(i);
    }
    else{
      document.getElementById(i+1).removeAttribute("class");
    }
  }
}

document.querySelector('.right-green').addEventListener('click', function() {
  // $( "div.playlist" ).scrollLeft( zx );
  // console.log ($( ".playlist ul" ).width());
  $('div.playlist').animate( { scrollLeft: '+=100' }, 500);
});
document.querySelector('.left-green').addEventListener('click', function() {
  // $( "div.playlist" ).scrollLeft( zx );
  // console.log ($( ".playlist ul" ).width());
  $('div.playlist').animate( { scrollLeft: '-=100' }, 500);
});


// document.querySelector('.hole-home').addEventListener('click', function() {
//   // $( "div.playlist" ).scrollLeft( zx );
//   $('div.playlist').animate( { scrollLeft: '-=1000' }, 500);
// });

